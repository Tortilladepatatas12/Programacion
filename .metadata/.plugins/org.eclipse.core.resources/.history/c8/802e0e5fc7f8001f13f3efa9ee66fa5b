package modelo;

public class Cliente {
    private String nombreCompleto;
    private String apellidosCompletos;
    private String dni;

    @Override
    public String toString() {
        return "Cliente [nombreCompleto=" + nombreCompleto + ", apellidosCompletos=" + apellidosCompletos + ", dni=" + dni + "]";
    }

    public Cliente(String nombreCompleto, String apellidosCompletos, String dni) {
        super();
        this.nombreCompleto = nombreCompleto;
        this.apellidosCompletos = apellidosCompletos;
        this.dni = dni;
    }
}
